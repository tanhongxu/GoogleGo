{
  "name": "Google Go!",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Translate google searched URL to its orginal URL.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "webRequest",
	"webRequestBlocking",
	"*://*.google.com/url*",
	"*://*.google.com.hk/url*",
	"*://*.google.com/search*",
	"*://*.google.com.hk/search*"
  ]
}